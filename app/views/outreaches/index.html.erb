<% content_for :j do %>
<script type="text/javascript">
	$(document).ready(function(){
	    $('#out').addClass('active');
	});
</script>
<% end %>

<% if user_signed_in? %>
<% if current_user.admin? %>
<div data-no-turbolink>
	<%= link_to("New Outreach Event", new_outreach_path) %>
</div>
<% end %>
<% end %>


<% if @outreaches.count == 0 %>
  <p style = "text-align:center;">No outreach events now, come back later.</p>
<% else %>
  <% @outreaches.each do |n| %>
  	<h2><%= n.title %></h2>
  	<div style = "margin-left: 50px;">
    	<%=raw n.content %>
    </div>
  <% end %>
<% end %>